        "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"!", 
          RowBox[{"ListQ", "[", "xTemp", "]"}]}], "&&", 
         RowBox[{"!", 
          RowBox[{"ListQ", "[", "yTemp", "]"}]}], "&&", 
         RowBox[{"!", 
          RowBox[{"ListQ", "[", "zTemp", "]"}]}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"az", "=", 
          RowBox[{"az", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], ";", " ", 
         RowBox[{"el", "=", 
          RowBox[{"el", "[", 
