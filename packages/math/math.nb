Cell["Function definitions", "Subsection",ExpressionUUID->"63506a71-b295-4cd6-94f6-58c0f409e49b"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"listDifferentiate", "[", "listData_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "xData", ",", "yData", ",", "xDataShiftLeft", ",", "yDataShiftLeft", 
       ",", "listDataLen", ",", "outputData"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xData", " ", "=", " ", 
       RowBox[{"listData", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"yData", " ", "=", " ", 
       RowBox[{"listData", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"xDataShiftLeft", " ", "=", " ", 
       RowBox[{"Drop", "[", 
        RowBox[{
         RowBox[{"RotateLeft", "[", 
          RowBox[{"xData", ",", "2"}], "]"}], ",", 
         RowBox[{"-", "2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"yDataShiftLeft", " ", "=", " ", 
       RowBox[{"Drop", "[", 
        RowBox[{
         RowBox[{"RotateLeft", "[", 
          RowBox[{"yData", ",", "2"}], "]"}], ",", 
         RowBox[{"-", "2"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"listDataLen", " ", "=", " ", 
       RowBox[{"Length", "[", "xData", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"outputData", " ", "=", " ", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", "listDataLen"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"outputData", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "=", 
       RowBox[{"N", "[", 
        FractionBox[
         RowBox[{
          RowBox[{"yData", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "-", 
          RowBox[{"yData", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], 
         RowBox[{
          RowBox[{"xData", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "-", 
          RowBox[{"xData", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}]], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"outputData", "[", 
        RowBox[{"[", 
         RowBox[{"2", ";;", 
          RowBox[{"listDataLen", "-", "1"}]}], "]"}], "]"}], "=", 
       RowBox[{"N", "[", 
        FractionBox[
         RowBox[{"yDataShiftLeft", "-", 
          RowBox[{"Drop", "[", 
           RowBox[{"yData", ",", 
            RowBox[{"-", "2"}]}], "]"}]}], 
         RowBox[{"xDataShiftLeft", "-", 
          RowBox[{"Drop", "[", 
           RowBox[{"xData", ",", 
            RowBox[{"-", "2"}]}], "]"}]}]], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"outputData", "[", 
        RowBox[{"[", "listDataLen", "]"}], "]"}], "=", 
       RowBox[{"N", "[", 
        FractionBox[
         RowBox[{
          RowBox[{"yData", "[", 
           RowBox[{"[", "listDataLen", "]"}], "]"}], "-", 
          RowBox[{"yData", "[", 
           RowBox[{"[", 
            RowBox[{"listDataLen", "-", "1"}], "]"}], "]"}]}], 
         RowBox[{
          RowBox[{"xData", "[", 
           RowBox[{"[", "listDataLen", "]"}], "]"}], "-", 
          RowBox[{"xData", "[", 
           RowBox[{"[", 
            RowBox[{"listDataLen", "-", "1"}], "]"}], "]"}]}]], "]"}]}], ";", 
      "\[IndentingNewLine]", "outputData"}]}], "\[IndentingNewLine]", "]"}]}],
   "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"listIntegrate", "[", "listdata_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "xdata", ",", "ydata", ",", "xdataShift", ",", "ydataShift", ",", 
       "totalIntegral"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xdata", " ", "=", " ", 
       RowBox[{"listdata", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ydata", " ", "=", " ", 
       RowBox[{"listdata", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"xdataShift", " ", "=", " ", 
       RowBox[{"Drop", "[", 
        RowBox[{
         RowBox[{"RotateLeft", "[", 
          RowBox[{"xdata", ",", "1"}], "]"}], ",", 
         RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ydataShift", " ", "=", " ", 
       RowBox[{"Drop", "[", 
        RowBox[{
         RowBox[{"RotateLeft", "[", 
          RowBox[{"ydata", ",", "1"}], "]"}], ",", 
         RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"totalIntegral", "=", 
       RowBox[{"N", "[", 
        RowBox[{"Total", "[", 
         RowBox[{
          RowBox[{"(", 
           FractionBox[
            RowBox[{
             RowBox[{"Drop", "[", 
              RowBox[{"ydata", ",", 
               RowBox[{"-", "1"}]}], "]"}], "+", "ydataShift"}], "2"], ")"}], 
          " ", 
          RowBox[{"(", 
           RowBox[{"xdataShift", "-", 
            RowBox[{"Drop", "[", 
             RowBox[{"xdata", ",", 
              RowBox[{"-", "1"}]}], "]"}]}], ")"}]}], "]"}], "]"}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"logMean", "[", 
    RowBox[{"f_", ",", "df_", ",", "Q_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "W", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"W", "=", 
       RowBox[{"Log", "[", 
        RowBox[{"10", ",", 
         FractionBox[
          RowBox[{"f", "+", 
           RowBox[{"df", "/", "2"}]}], 
          RowBox[{"f", "-", 
           RowBox[{"df", "/", "2"}]}]]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Total", "[", 
        RowBox[{"W", " ", "Q"}], "]"}], "/", 
       RowBox[{"Total", "[", "W", "]"}]}]}]}], "\[IndentingNewLine]", "]"}]}],
   "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"logSTD", "[", 
   RowBox[{"f_", ",", "df_", ",", "Q_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"W", ",", "Qbar"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"W", "=", 
      RowBox[{"Log", "[", 
       RowBox[{"10", ",", 
        FractionBox[
         RowBox[{"f", "+", 
          RowBox[{"df", "/", "2"}]}], 
         RowBox[{"f", "-", 
          RowBox[{"df", "/", "2"}]}]]}], "]"}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"Qbar", " ", "=", " ", 
      RowBox[{"logMean", "[", 
       RowBox[{"f", ",", "df", ",", "Q"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"Total", "[", 
        RowBox[{"W", " ", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"Q", "-", "Qbar"}], ")"}], "2"]}], "]"}], "/", 
       RowBox[{"Total", "[", "W", "]"}]}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",
